#: dpkgunlocker-gui/rsrc/ListDelegateServiceItem.qml:80
msgid "Unlocked"
msgstr "Desbloqueado"

#: dpkgunlocker-gui/rsrc/ListDelegateServiceItem.qml:83
msgid "Locked: Currently executing"
msgstr "Bloqueado: Actualmente esta siendo ejecutado"

#: dpkgunlocker-gui/rsrc/ListDelegateServiceItem.qml:86
msgid "Locked: Not process found"
msgstr "Bloqueado: No se ha encontrado ningún proceso"

#: dpkgunlocker-gui/rsrc/ListDelegateServiceItem.qml:89
