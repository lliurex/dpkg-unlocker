msgstr "Procés de desbloqueig"

#: dpkgunlocker-gui/python3-dpkgunlocker-gui/rsrc/ApplicationOptions.qml:49
msgid "Metapackage protection"
msgstr "Protecció del metapaquet"

#: dpkgunlocker-gui/python3-dpkgunlocker-gui/rsrc/ApplicationOptions.qml:61
msgid "Help"
msgstr "Ajuda"

#: dpkgunlocker-gui/python3-dpkgunlocker-gui/rsrc/ApplicationOptions.qml:130
